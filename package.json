{
    "name": "coup",
    "version": "1.0.0",
    "private": false,
    "description": "Coup online, with support for custom variants",
    "author": "Jeffrey Meng",
    "keywords": [
        "gatsby"
    ],
    "babelMacros": {
        "twin": {
            "preset": "emotion"
        }
    },
    "scripts": {
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write --ignore-unknown .",
        "prepare": "husky install",
        "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ."
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --fix --ext .js,.jsx,.ts,.tsx",
        "*": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@headlessui/react": "^1.6.4",
        "@heroicons/react": "^1.0.6",
        "@mdx-js/mdx": "^1.6.22",
        "@mdx-js/react": "^1.6.22",
        "gatsby": "^4.16.0",
        "gatsby-plugin-emotion": "^7.16.0",
        "gatsby-plugin-mdx": "^3.16.1",
        "gatsby-plugin-react-helmet": "^5.16.0",
        "gatsby-source-filesystem": "^4.16.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-helmet": "^6.1.0"
    },
    "devDependencies": {
        "@fontsource/inter": "^4.5.11",
        "@tailwindcss/typography": "^0.5.2",
        "@types/node": "^17.0.42",
        "@types/react": "^18.0.12",
        "@types/react-dom": "^18.0.5",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "eslint": "^8.17.0",
        "eslint-plugin-react": "^7.30.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.1",
        "prettier": "2.6.2",
        "tailwindcss": "^3.1.2",
        "twin.macro": "^2.8.2",
        "typescript": "^4.7.3"
    }
}
